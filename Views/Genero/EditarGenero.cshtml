@model casaDeShows.Models.GeneroEvento
<h3>Editar genero musical</h3>
<hr />
<div class="row">
    <div class="col-md-5">
        <form asp-controller="Genero" asp-action="EditarGenero" method="POST">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Genero" class="titles"></label>
                <input asp-for="Genero" class="form-control" />
                <span asp-validation-for="Genero" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" id="btnsucesso" value="Create" class="btn btn-success"/>
            </div>
        </form>
    </div>
</div>